@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Races on Map";
    var Races = (List<Race>)ViewData["races"]!;
}

<img src="~/images/banners/map.png" usemap="#racesmap" />

<map name="racesmap">
    @foreach (var race in Races)
    {
        <a asp-action="ShowRace" asp-route-id="@race.RaceID">
            <area shape="circle" coords="@race.X,@race.Y,20" />
        </a>

    }

</map>